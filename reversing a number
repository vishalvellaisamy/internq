#include <iostream>
using namespace std;

int main()
{
   //reversing a number
   int num,rem;
   int revnum=0;
   cout<<"enter any number:";
   cin>>num;
   while (num!=0){
       rem=num%10;
       revnum=revnum*10+rem;
       num=num/10;
       
   }
   cout<<"the reverse num is"<<revnum;
    
    return 0;
}
